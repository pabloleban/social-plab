{
  "name": "social-plab",
  "version": "1.3.6",
  "description": "Create easy images for Instagram",
  "main": "main.js",
  "build": {
    "publish": {
      "provider": "github",
      "releaseType": "release"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Social Plab"
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64",
    "gh-publish": "electron-builder --x64 -p=always"
  },
  "repository": "https://github.com/pabloleban/social-plab",
  "keywords": [
    "image",
    "social",
    "generator",
    "instagram"
  ],
  "author": "Pablo Leban",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.10",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.2",
    "bootstrap": "^4.3.1",
    "electron-reload": "^1.5.0",
    "electron-titlebar-windows": "^3.0.0",
    "electron-updater": "^4.1.2",
    "fs-plus": "^3.1.1",
    "html-to-image": "^0.1.1",
    "jquery": "^3.4.1",
    "jquery-textfill": "^0.6.0",
    "jquery-ui-dist": "^1.12.1",
    "lato-font": "^3.0.0",
    "node-fetch": "^2.6.0",
    "popper.js": "^1.15.0",
    "sweetalert2": "^8.16.3"
  }
}
