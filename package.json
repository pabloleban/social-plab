{
  "name": "social-plab",
  "version": "1.1.0",
  "description": "Create easy images for Instagram",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64",
    "gh-publish": "electron-builder --x64 -p always",
    "package-win": "electron-packager . \"social-plab\" --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=\"dist\" --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Social Plab\" --win32metadata.ProductName=\"Social Plab\"",
    "create-win-installer": "electron-installer-windows --src dist/social-plab-win32-ia32/ --dest dist/installers/"
  },
  "repository": "https://github.com/pabloleban/social-plab",
  "keywords": [
    "image",
    "social",
    "generator",
    "instagram"
  ],
  "author": "Pablo Leban",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.3",
    "electron-builder": "^21.2.0",
    "electron-installer-windows": "^2.0.0",
    "electron-packager": "^13.1.1",
    "electron-updater": "^4.1.2"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-titlebar-windows": "^3.0.0",
    "fs-plus": "^3.1.1",
    "html-to-image": "^0.1.0",
    "jquery": "^3.4.1",
    "jquery-ui-dist": "^1.12.1",
    "node-fetch": "^2.6.0",
    "popper.js": "^1.15.0",
    "sweetalert2": "^8.12.1"
  }
}
